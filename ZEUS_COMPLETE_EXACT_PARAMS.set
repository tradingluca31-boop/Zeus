InpSL_PercentOfPrice=0.35
InpTP_PercentOfPrice=1.75
InpBE_TriggerPercent=1.0
InpMaxTradesPerDay=4

; Fenêtre d'ouverture (EXACT)
InpSignalTF=16385
InpSessionStartHour=6
InpSessionEndHour=15
InpSlippagePoints=20
InpVerboseLogs=false

; SMMA50 + Score conditions (EXACT)
InpUseSMMA50Trend=true
InpSMMA_Period=50
InpSMMA_TF=16388

; RSI Filter (EXACT)
InpUseRSI=true
InpRSITF=16388
InpRSIPeriod=14
InpRSIOverbought=70
InpRSIOversold=25
