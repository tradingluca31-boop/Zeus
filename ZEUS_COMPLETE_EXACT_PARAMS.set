; ZEUS USD V2 COMPLETE - Paramètres EXACTS du code Poseidon
; Reprend TOUS les paramètres et filtres identiques

InpMagic=20250930
InpAllowBuys=true
InpAllowSells=true

; Export CSV
InpAutoExportOnDeinit=true
InpExportStartDate=2000.01.01 00:00:00
InpExportEndDate=2045.01.01 00:00:00
InpFileSuffix=ZEUS_COMPLETE

; Sélection des paires USD (toutes activées)
InpTrade_EURUSD=true
InpTrade_GBPUSD=true
InpTrade_USDJPY=true
InpTrade_USDCHF=true
InpTrade_AUDUSD=true
InpTrade_NZDUSD=true
InpTrade_USDCAD=true

; 3 Signaux indépendants (EXACT comme Poseidon)
InpUseEMA_Cross=true
InpUseMACD=true
InpUseSMMA_Cross=true
InpMinSignalsRequired=2

; MACD SMA config (EXACT)
InpMACD_Fast=20
InpMACD_Slow=35
InpMACD_Signal=15

; Risque / gestion (EXACT)
InpRiskPercent=1.0
UseLossStreakReduction=true
LossStreakTrigger=7
LossStreakFactor=0.50

; RISQUE EN MONTANT FIXE (EXACT)
